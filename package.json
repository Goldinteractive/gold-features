{
  "name": "gold-features",
  "version": "0.0.1-alpha",
  "description": "contains all gold features",
  "author": "Gold Interactive",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Goldinteractive/gold-features.git"
  },
  "scripts": {
    "lerna": "lerna",
    "lerna:build": "lerna run build",
    "lerna:updated": "lerna updated",
    "lerna:publish": "yarn --frozen-lockfile && yarn lerna:build && yarn lerna publish",
    "lerna:exec": "lerna exec --",
    "stylemark": "node scripts/docs.js && stylemark -i packages -o docs -c stylemark.config.yml",
    "stylemark:watch": "node scripts/docs.js && stylemark -i packages -o docs -c stylemark.config.yml -w",
    "stylemark:publish": "./publish-docs.sh",
    "watch": "node scripts/watch-project.js",
    "create:feature": "node scripts/create.js",
    "format": "prettier --write \"**/*.{js,jsx,json}\" \"!**/helpers/skeleton/**/*\" \"!docs/**/*\" \"!**/lib/**/*\""
  },
  "devDependencies": {
    "@goldinteractive/ignore-assets-webpack-plugin": "^3.0.0",
    "autoprefixer": "^7.2.5",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.6.1",
    "chalk": "^2.3.0",
    "css-loader": "^1.0.0",
    "cssnano": "^3.10.0",
    "extract-loader": "^2.0.1",
    "file-loader": "^1.1.11",
    "lerna": "3.0.0-beta.21",
    "minimist": "^1.2.0",
    "mkdirp": "^0.5.1",
    "ncp": "^2.0.0",
    "node-sass": "^4.9.2",
    "ow": "^0.6.0",
    "postcss": "^6.0.17",
    "postcss-cli": "^5.0.0",
    "postcss-import": "^11.0.0",
    "postcss-loader": "^2.1.6",
    "postcss-pxtorem": "^4.0.1",
    "prettier": "^1.13.6",
    "replace-in-file": "^3.1.1",
    "run-parallel": "^1.1.6",
    "sass-loader": "^7.0.3",
    "shelljs": "^0.8.1",
    "stylemark": "^3.0.2",
    "uppercamelcase": "^3.0.0",
    "webpack": "^4.16.1",
    "webpack-format-messages": "^2.0.1"
  },
  "browserslist": [
    "> 5%",
    "last 2 version"
  ],
  "dependencies": {}
}
